<table class="table-auto shadow mb-10">
  <thead>
  <tr class="bg-slate-200 rounded-t-lg border border-slate-500">
    {{#each users.[0]}}
      <th class="py-1 px-3">{{@key}}</th>
    {{/each}}
  </tr>
  </thead>
  <tbody class="border border-slate-500">
  {{#each users}}
    <tr>
      {{#each this}}
        <td class="py-1 px-3">{{this}}</td>
      {{/each}}
    </tr>
  {{/each}}
  </tbody>
</table>
<a class="text-blue-500 hover:text-blue-700 underline transition" href="/">Back</a>
